					"listen": "test",
					"script": {
						"exec": [
							"// 提取json格式的响应数据\r",
							"var jsonData = pm.response.json();\r",
							"\r",
							"// 提取图片的URL\r",
							"var url = jsonData.url;\r",
							"\r",
							"// 把提取出来的图片保存到全局变量\r",
							"pm.globals.set(\"g_url\", url);\r",
							"\r",
							"// 把提取出来的图片保存到环境变量\r",
							"pm.environment.set(\"e_url\", url);\r",
							"\r",
							"pm.test(\"Status code is 200\", function () {\r",
