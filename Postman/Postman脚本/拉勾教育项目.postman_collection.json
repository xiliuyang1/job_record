					"script": {
						"exec": [
							"// 断言响应状态码是不是200\r",
							"pm.test(\"Status code is 200\", function () {\r",
							"    pm.response.to.have.status(200);\r",
							"});\r",
							"\r",
							"// 断言响应数据中，响应体是否包含手机号码15321919666\r",
							"pm.test(\"响应体是否包含手机号码15321919666\", function () {\r",
							"    pm.expect(pm.response.text()).to.include(\"15321919666\");\r",
							"});// pm.response.text() 获取文本格式的响应体\r",
							"\r",
							"// 断言拉勾教育登陆接口中，id的值是不是10030011\r",
							"pm.test(\"断言拉勾教育登陆接口中，id的值是不是10030011\", function () {\r",
							"    var aaa = pm.response.json();\r",
							"    pm.expect(aaa.content.user.id).to.eql(100030011);\r",
							"});\r",
							"\r",
							"// 断言响应体：断言填写的预期的响应体，与实际的返回的响应体是否一致\r",
							"pm.test(\"Body is correct\", function () {\r",
							"    pm.response.to.have.body({\r",
							"    \"success\": true,\r",
							"    \"state\": 1,\r",
							"    \"message\": \"响应成功\",\r",
							"    \"content\": {\r",
							"        \"access_token\": \"7db00cab-1d21-4f3c-839c-2dc4075b94ce\",\r",
							"        \"user_id\": 100030011,\r",
							"        \"user\": {\r",
							"            \"id\": 100030011,\r",
							"            \"name\": \"15321919666\",\r",
							"            \"portrait\": \"https://edu-lagou.oss-cn-beijing.aliyuncs.com/images/2020/06/28/15933251448762927.png\",\r",
							"            \"phone\": \"15321919666\",\r",
							"            \"password\": \"123456\",\r",
							"            \"reg_ip\": null,\r",
							"            \"account_non_expired\": null,\r",
							"            \"credentials_non_expired\": null,\r",
							"            \"account_non_locked\": null,\r",
							"            \"status\": \"ENABLE\",\r",
							"            \"is_del\": null,\r",
							"            \"createTime\": 1594347555000,\r",
							"            \"updateTime\": 1594347555000\r",
							"        }\r",
							"    }\r",
							"});\r",
							"});\r",
							"\r",
							"// 断言响应头\r",
							"pm.test(\"Server is present\", function () {\r",
							"    pm.response.to.have.header(\"Server\",\"Apache-Coyote/1.1\");\r",
							"});\r",
							"\r",
							"// 断言响应时间不超过300毫秒\r",
							"pm.test(\"Response time is less than 300ms\", function () {\r",
							"    pm.expect(pm.response.responseTime).to.be.below(300);\r",
							"});\r",
							"\r",
							"\r",
							""
